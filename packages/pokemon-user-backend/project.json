{
  "name": "pokemon-user-backend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/pokemon-user-backend/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "pokemon-user-backend:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "pokemon-user-backend:build:development"
        },
        "production": {
          "buildTarget": "pokemon-user-backend:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/pokemon-user-backend"],
      "options": {
        "jestConfig": "packages/pokemon-user-backend/jest.config.ts"
      }
    },
    "test:watch": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "packages/pokemon-user-backend/jest.config.ts",
        "watch": true
      }
    },
    "test:coverage": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "packages/pokemon-user-backend/jest.config.ts",
        "coverage": true,
        "coverageReporters": ["text", "lcov", "html"]
      }
    }
  }
}
